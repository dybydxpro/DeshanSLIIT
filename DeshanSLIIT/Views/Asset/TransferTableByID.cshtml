@model IEnumerable<Transfer>
@{
    ViewData["Title"] = "TransferTableByID";
}

<h1>TransferTableByID</h1>

<table class="table table-danger table-hover">
    <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">TID</th>
          <th scope="col">CartID</th>
          <th scope="col">Name</th>
          <th scope="col">ISBN</th>
          <th scope="col">Location</th>
          <th scope="col">Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
              <th scope="row">@data.Id</th>
              <td>@data.TID</td>
              <td>@data.CartID</td>
              <td>@data.Name</td>
              <td>@data.ISBN</td>
              <td>@data.Location</td>
              <td>@data.Date</td>
            </tr>
        }
    </tbody>
</table>